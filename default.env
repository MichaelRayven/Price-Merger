export DRIVER_PATH='' # Path to a folder with a ChromeDriver, download here: https://chromedriver.storage.googleapis.com/index.html

export URL_LIST_PATH=""  # (Optional) Provide a path to a .json list of product urls to use instead of search query
export SEARCH_QUERY=""  # A https://www.e-katalog.ru/ product search query
export URLS_QUANTITY=100  # A desirable amount of URL to gather data from

# Defines the value of the graphs used for analysis
export DATA_PREFERENCE="Avg"  # MIN (Minimun), AVG (Average), MAX (Maximum) defines price type used to make the graph
export DATA_THRESHOLD=50  # A minimun amount of price data for the point to be included in the graph